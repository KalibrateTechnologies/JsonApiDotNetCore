{
    "metadata": [
        {
            "src": [
                {
                    "files": [ "**.csproj" ],
                    "src": "C:\\dev\\JsonApiDotNetCore\\src\\JsonApiDotNetCore"
                }
            ],
            "dest": "api",
            "disableGitFeatures": false,
            "properties": {
                "targetFramework": "netcoreapp3.1"
            }
        }
    ],
    "build": {
        "content": [
            {
                "files": [ "api/**.yml", "api/index.md" ]
            },
            {
                "files": [
                    "articles/**.md",
                    "articles/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [ "images/**" ]
            }
        ],
        "overwrite": [
            {
                "files": [ "apidoc/**.md" ],
                "exclude": [ "obj/**", "_site/**" ]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [ "default" ],
        "postProcessors": [],
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}
